package com.example.budgetnotebook;

import android.test.ActivityInstrumentationTestCase2;
import android.text.Layout;
import android.view.View;
//import junit.framework.TestCase;
import android.widget.TextView;

public class MainMenuValidation extends ActivityInstrumentationTestCase2<MainMenu> {
	//MainMenu activity;
	//boolean j = true;
	private MainMenu MainMenuActivity;
    private View MainMenuView;
    
	public MainMenuValidation() {
		super(MainMenu.class);
		//super("src.com.example.budgetnotebook.MainMenu");
		//setName(name);
		//assert j = true;
	}
	
	@Override
    protected void setUp() throws Exception {
        super.setUp();
        MainMenuActivity = getActivity();
        MainMenuView = (View) MainMenuActivity.findViewById(R.layout.main_menu);
	}
	
	public void testMainMenu_initialView() {
	    final View expected = MainMenuActivity.findViewById(R.layout.main_menu;
	    final View actual = MainMenuView;
	    assertEquals(expected, actual);
	}

}
